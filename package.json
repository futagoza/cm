{
    "name": "cm",
    "version": "0.0.0-dev.4-wip",
    "private": true,
    "main": "src/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "setup": "npm install && cd dist && npm install",
        "prerelease": "node scripts/ci-check",
        "release": "node scripts/release",
        "postrelease": "node scripts/publish",
        "rebuild": "npm run clean && npm run build",
        "build": "run-s compile:*",
        "compile:pegjs": "node scripts/build-parser",
        "compile:esnext": "node scripts/transpile",
        "lint": "eslint --cache bin scripts src",
        "benchmark": "node scripts/benchmark",
        "spec": "node scripts/spec-runner",
        "test": "run-s \"lint -- --quiet\" benchmark spec",
        "clean": "node scripts/remove"
    },
    "dependencies": {
        "npm-run-all": "4.0.2",
        "markdown-toc": "1.1.0",
        "eslint": "3.19.0",
        "eslint-config-futagozaryuu": "2.1.0",
        "babel-core": "6.24.1",
        "babel-preset-env": "1.4.0",
        "babel-preset-stage-0": "6.24.1",
        "babel-plugin-transform-runtime": "6.23.0",
        "jsdoc": "3.4.3",
        "minami": "1.1.1",
        "benchr": "3.4.0",
        "mocha": "3.2.0",
        "conventional-changelog-cli": "1.3.1",
        "linefix": "0.1.1",
        "mkdirp": "0.5.1",
        "rimraf": "2.6.1"
    },
    "engines": {
        "node": ">= 4.0.0"
    }
}
